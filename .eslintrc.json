{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "unused-imports"],
  "rules": {
    "@next/next/no-img-element": "off",
    "react-hooks/exhaustive-deps": "off",
    "unused-imports/no-unused-imports": "error"
  }
}
